{
  "name": "time-control-backend",
  "version": "1.0.0",
  "description": "",
  "main": "Server.ts",
  "scripts": {
    "dev": "ts-node-dev --env-file=.env ./src/Server.ts",
    "start": "node ./build/index.js",
    "test": "jest",
    "build": "tsc --build",
    "test:silent": "jest --watchAll --silent --noStackTrace",
    "knex:migrate": "knex --knexfile ./src/database/knex/Environment.ts migrate:latest",
    "knex:rollback": "knex --knexfile ./src/database/knex/Environment.ts migrate:rollback",
    "knex:rollback-all": "knex --knexfile ./src/database/knex/Environment.ts migrate:rollback -all",
    "knex:seed": "knex --knexfile ./src/database/knex/Environment.ts seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/express": "^4.17.21",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.57.0",
    "globals": "^15.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript-eslint": "^7.9.0"
  },
  "dependencies": {
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0",
    "knex": "^3.1.0",
    "yup": "^1.4.0"
  }
